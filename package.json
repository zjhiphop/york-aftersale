{
  "name": "after-sale",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "babel-plugin-import": "^1.4.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "concurrently": "^3.5.0",
    "metro-bundler": "^0.18.0",
    "react": "^16.0.0-alpha.12",
    "react-dom": "^16.0.0-alpha.12",
    "react-native-typescript-transformer": "^1.1.4",
    "react-test-renderer": "16.0.0-alpha.12",
    "rimraf": "^2.6.2",
    "rn-nodeify": "^8.0.0",
    "ts-jest": "^21.0.1",
    "tslib": "^1.7.1",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf dist/**/*.js dist/**/*.map",
    "build": "npm run clean && npm run tsc --",
    "start": "npm run build  && concurrently -r 'npm run watch' 'react-native start'",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js --watch  --coverage",
    "lint": "tslint src/**/*.ts",
    "watch": "npm run build -- -w",
    "start:ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
    "start:android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'",
    "postinstall": "./node_modules/.bin/rn-nodeify --install net,buffer,events,process,stream,util,inherits,fs,path --hack"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "@types/jest": "^20.0.8",
    "@types/react": "^16.0.5",
    "@types/react-native": "^0.48.6",
    "@types/react-test-renderer": "^15.5.4",
    "antd-mobile": "^2.0.0-beta.2",
    "buffer": "^4.9.1",
    "moment": "^2.19.0",
    "path-browserify": "0.0.0",
    "process": "^0.11.0",
    "rc-form": "^1.4.8",
    "react": "^16.0.0-alpha.12",
    "react-moment": "^0.6.5",
    "react-native": "^0.47.0",
    "react-native-event-listeners": "^1.0.3",
    "react-native-level-fs": "^3.0.0",
    "react-native-open-navi": "^1.0.0",
    "react-native-paho-mqtt": "^0.1.1",
    "react-native-phone-call": "^1.0.4",
    "react-native-splash-screen": "^3.0.1",
    "react-native-tcp": "^3.2.1",
    "react-native-timeline-listview": "^0.2.2",
    "react-navigation": "^1.0.0-beta.11",
    "readable-stream": "1.0.33",
    "stream": "0.0.2",
    "stream-browserify": "^1.0.0",
    "util": "~0.10.3",
    "websocket": "^1.0.24"
  },
  "react-native": {
    "net": "react-native-tcp",
    "path": "path-browserify",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "browser": {
    "net": "react-native-tcp",
    "path": "path-browserify",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  }
}
